{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "nicName": {
            "value": "ServicesNIC"
        },
        "nsgName": {
            "value": "ServicesNSG"
        },
        "publicIpAddressName": {
            "value": "ServicesPIP#{Octopus.Deployment.Tenant.Name}"
        },
        "vmAdminUsername": {
            "reference": {
                "keyVault": {
                    "id": "#{KeyVaultResourceId}"
                },
                "secretName": "VMAdminUsername"
            }
        },
        "vmAdminPassword": {
            "reference": {
                "keyVault": {
                    "id": "#{KeyVaultResourceId}"
                },
                "secretName": "VMAdminPassword"
            }
        },
        "clientID": {
            "value": "#{OctopusAzureADClientId}"
        },
        "clientSecret": {
            "value": "#{OctopusAzureADPassword}"
        },
        "vmName": {
            "value": "ServicesWeb"
        },
        "keyVaultName": {
            "value": "#{KeyVaultName}"
        },
        "registrationKey": {
            "reference": {
                "keyVault": {
                    "id": "#{KeyVaultResourceId}"
                },
                "secretName": "AutomationRegistrationKey"
            }
        },
        "registrationUrl": {
            "reference": {
                "keyVault": {
                    "id": "#{KeyVaultResourceId}"
                },
                "secretName": "AutomationRegistrationUrl"
            }
        },
        "nodeConfigurationName": {
            "value": "Services.Frontend"
        },
        "timestamp": {
            "value": "#{Octopus.Deployment.CreatedUtc}"
        },
        "servicesResourceGroupName": {
            "value": "#{ServicesResourceGroupName}"
        }
    }
}