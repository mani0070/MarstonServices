{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "trafficManagerName": {
            "value": "#{TrafficManagerName}"
        },
        "trafficManagerRelativeName": {
            "value": "#{TrafficManagerRelativeName}"
        },
        "bluePublicIpResourceId": {
            "value": "#{Octopus.Action[Public IP - Blue - Reverse FQDN].Output.AzureRMOutputs[resourceId]}"
        },
        "bluePublicIpFqdn": {
            "value": "#{Octopus.Action[Public IP - Blue - Reverse FQDN].Output.AzureRMOutputs[fqdn]}"
        },
        "blueStatus": {
            "value": "#{TrafficManagerBlueStatus}"
        },
        "bluePriority": {
            "value": "#{TrafficManagerBluePriority}"
        },
        "greenPublicIpResourceId": {
            "value": "#{Octopus.Action[Public IP - Green - Reverse FQDN].Output.AzureRMOutputs[resourceId]}"
        },
        "greenPublicIpFqdn": {
            "value": "#{Octopus.Action[Public IP - Green - Reverse FQDN].Output.AzureRMOutputs[fqdn]}"
        },
        "greenStatus": {
            "value": "#{TrafficManagerGreenStatus}"
        },
        "greenPriority": {
            "value": "#{TrafficManagerGreenPriority}"
        }
    }
}